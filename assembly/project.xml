<?xml version="1.0" encoding="UTF-8"?>
<project>
  <extend>../etc/project.xml</extend>
  <id>assembly</id>
  <name>assembly</name>
  <dependencies>
    <dependency>
      <groupId>geronimo-devtools</groupId>
      <artifactId>org.apache.geronimo.devtools.eclipse.core</artifactId>
      <version>${devtools_currentversion}</version>
      <properties>
        <jar>true</jar>
      </properties>
    </dependency>
    <dependency>
      <groupId>geronimo-devtools</groupId>
      <artifactId>org.apache.geronimo.deployment.model</artifactId>
      <version>${devtools_currentversion}</version>
      <properties>
        <jar>true</jar>
      </properties>
    </dependency>
    <dependency>
      <groupId>geronimo-devtools</groupId>
      <artifactId>org.apache.geronimo.feature</artifactId>
      <version>${devtools_currentversion}</version>
      <properties>
        <feature>true</feature>
      </properties>
    </dependency>
    <dependency>
      <groupId>geronimo-devtools</groupId>
      <artifactId>org.apache.geronimo.runtime.v1</artifactId>
      <version>${devtools_currentversion}</version>
      <properties>
        <jar>false</jar>
      </properties>
    </dependency>
    <dependency>
      <groupId>geronimo-devtools</groupId>
      <artifactId>org.apache.geronimo.ui</artifactId>
      <version>${devtools_currentversion}</version>
      <properties>
        <jar>true</jar>
      </properties>
    </dependency>
  </dependencies>
</project>
